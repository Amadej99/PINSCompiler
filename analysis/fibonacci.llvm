declare printf(_ : string, ...) : integer;

fun main(_ : integer) : integer =
(
    printf('%d\n', fib(6)),
    0
);

fun fib(n : integer) : integer =
(
    {
        if n == 0 | n == 1 then
            { x = 1 }
        else
            { x = fib(n - 1) + fib(n - 2) }
    },
    x
) { where var x : integer }